{
  "grammars": [
    {
      "name": "jake",
      "camelcase": "Jake",
      "scope": "source.jake",
      "path": ".",
      "file-types": [
        "jake",
        ".jake",
        "jakefile",
        "JAKEFILE",
        "Jakefile",
        ".jakefile",
        ".JAKEFILE",
        ".Jakefile"
      ],
      "highlights": "queries-src/highlights.scm",
      "injections": "queries-src/injections.scm",
      "locals": "queries-src/locals.scm",
      "injection-regex": "^(?i)jake(file)?$",
      "first-line-regex": "#!\\S*bin\\S*[/ ]jake"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Jake grammar for tree-sitter",
    "authors": [
      {
        "name": "Jake contributors"
      }
    ],
    "links": {
      "repository": "https://github.com/helgee/jake"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
